{
  "name": "salfagpt",
  "version": "0.3.0",
  "type": "module",
  "private": false,
  "description": "Flow AI Platform - Developer CLI and SDK for context management",
  "keywords": [
    "ai",
    "context-management",
    "document-processing",
    "cli",
    "sdk"
  ],
  "author": "Alec <alec@getaifactory.com>",
  "license": "MIT",
  "bin": {
    "salfagpt": "./dist/cli/index.js"
  },
  "scripts": {
    "load-env": "node scripts/load-env.js",
    "dev": "npm run load-env && astro dev",
    "build": "astro build && node scripts/fix-production-paths.js",
    "build:local": "npm run load-env && astro build",
    "preview": "astro preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.astro",
    "build:cli": "tsc cli/index.ts --outDir dist/cli --module esnext --target es2022 --moduleResolution bundler",
    "prepublishOnly": "npm run build:cli",
    "verify:agents": "tsx scripts/verify-agents.ts",
    "fix:mark-agents": "tsx scripts/fix-mark-agents.ts",
    "migrate:user-ids": "tsx scripts/migrate-users-to-hash-ids.mjs",
    "migrate:user-ids:execute": "tsx scripts/migrate-users-to-hash-ids.mjs --execute",
    "migrate:all-users": "tsx scripts/migrate-all-user-formats.mjs",
    "migrate:all-users:execute": "tsx scripts/migrate-all-user-formats.mjs --execute",
    "find:alec-convs": "tsx scripts/find-alec-conversations.mjs",
    "check:alec-data": "tsx scripts/check-alec-complete-data.mjs",
    "check:folders": "tsx scripts/check-folders-userid.mjs",
    "fix:folders": "tsx scripts/fix-folders-userid.mjs",
    "fix:folders:execute": "tsx scripts/fix-folders-userid.mjs --execute",
    "test:mcp": "tsx scripts/test-mcp-server.ts",
    "seed:changelog": "tsx scripts/seed-changelog.ts",
    "seed:changelog:enhanced": "tsx scripts/seed-changelog-enhanced.ts",
    "roadmap:add-changelog": "tsx scripts/add-changelog-to-roadmap.ts",
    "init:onboarding": "tsx scripts/init-feature-onboarding.ts",
    "migrate:multi-org": "tsx scripts/migrate-to-multi-org.ts",
    "migrate:multi-org:dry-run": "tsx scripts/migrate-to-multi-org.ts --dry-run",
    "staging:setup": "bash scripts/create-staging-mirror.sh",
    "qa:setup": "bash scripts/setup-qa-environment.sh",
    "qa:deploy": "bash scripts/deploy-to-qa.sh",
    "qa:compare": "bash scripts/compare-qa-prod.sh",
    "verify:userid-formats": "tsx scripts/verify-userid-formats.ts",
    "discover:userid-mappings": "tsx scripts/discover-userid-mappings.ts",
    "migrate:userid": "tsx scripts/migrate-userid-format.ts",
    "qa:status": "bash scripts/status.sh",
    "prod:deploy": "bash scripts/deploy-to-production.sh",
    "prod:rollback": "bash scripts/rollback-production.sh",
    "deploy:track": "bash scripts/track-deployment.sh"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/node": "^9.0.0",
    "@astrojs/react": "^3.6.2",
    "@astrojs/tailwind": "^6.0.2",
    "@google-cloud/aiplatform": "^5.11.0",
    "@google-cloud/bigquery": "^7.9.0",
    "@google-cloud/documentai": "^9.5.0",
    "@google-cloud/error-reporting": "^3.0.5",
    "@google-cloud/firestore": "^7.10.0",
    "@google-cloud/logging": "^11.2.1",
    "@google-cloud/storage": "^7.14.0",
    "@google-cloud/vertexai": "^1.10.0",
    "@google-cloud/vision": "^5.3.4",
    "@google/genai": "^1.23.0",
    "astro": "^5.1.2",
    "date-fns": "^4.1.0",
    "google-auth-library": "^10.4.1",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.468.0",
    "mammoth": "^1.11.0",
    "papaparse": "^5.4.1",
    "pdf-lib": "^1.17.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.6.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "tailwindcss": "3.4.17",
    "xlsx": "^0.18.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@types/react-syntax-highlighter": "^15.5.13",
    "dotenv": "^17.2.3",
    "firebase-admin": "^12.7.0",
    "tsx": "^4.20.6",
    "typescript": "^5.7.2"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=20.0.0"
  }
}
