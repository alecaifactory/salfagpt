{
  "name": "salfagpt",
  "version": "0.1.0",
  "description": "SalfaGPT - Enterprise AI Platform by AI Factory",
  "type": "module",
  "scripts": {
    "dev": "astro dev --host 0.0.0.0",
    "build": "astro build",
    "preview": "astro preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "test:hiab": "npx tsx scripts/test-single-hiab.ts",
    "test:scania": "npx tsx scripts/test-scania-large.ts",
    "test:s2-bulk": "npx tsx scripts/test-s2-bulk-upload.ts",
    "deploy:dev": "gcloud run deploy cr-salfagpt-ai-ft-dev --source . --region us-east4 --project salfagpt --allow-unauthenticated",
    "deploy:prod": "gcloud run deploy cr-salfagpt-ai-ft-prod --source . --region us-east4 --project salfagpt --allow-unauthenticated",
    "setup:python": "pip3 install PyPDF2"
  },
  "dependencies": {
    "@astrojs/node": "^9.0.0",
    "@astrojs/react": "^4.0.1",
    "@astrojs/tailwind": "^5.1.3",
